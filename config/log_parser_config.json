{
    "log_validation": {
        "markers": [
            "[UNITYVERSION]",
            "Running as SYSTEM"
        ],
        "min_matches": 1,
        "scan_size_kb": 4
    },
    "file_reading": {
        "default_chunk_size": 8192,
        "default_encoding": "utf-8",
        "supported_encodings": [
            "utf-8",
            "utf-8-sig",
            "gbk",
            "utf-16"
        ],
        "encoding_confidence_threshold": 0.7
    }
}